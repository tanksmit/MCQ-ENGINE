{
    "version": 2,
    "cleanUrls": true,
    "functions": {
        "server/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ]
}